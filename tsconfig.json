{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@tensorflow/tfjs": ["types/blank"],
      "@tensorflow-models/face-landmarks-detection": ["types/blank"],
      "react/compiler-runtime": ["types/blank"],
      "recharts": ["types/blank"],
      "@supabase/auth-ui-react": ["types/blank"],
      "@supabase/auth-ui-shared": ["types/blank"],
      "@stitches/core/types/styled-component": ["types/blank"],
      "@supabase/auth-helpers-nextjs": ["types/blank"]
    },

    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "target": "ES2020",
    "skipLibCheck": false,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{ "name": "next" }]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules", "supabase/functions"]
}
